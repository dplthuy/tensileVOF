FoamFile
{
    version     2.0;
    format      ascii;
    class       IOobject;
    location    "0";
    object      air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

MWGas       28.9;  // Molecular weight of the gas
CpGas       1007;  // Heat capacity of the gas
HfGas       0;  // Enthalpy of formation
muGas       1.84e-5;  // Dynamic viscosity of the gas
PrGas       0.7; // Prandtl number of the gas

