FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include "./initialConditions"

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
    gasInlet
    {
        type        fixedValue;
        value		uniform 0.0005;
    }
    meltInlet 
    {
    	type		fixedValue;
	    value		uniform 0.0001;
    }
    cylinder
    {
    	type		zeroGradient;
    }
    outlet
    {
    	type		zeroGradient;
    }
    symmetry1
    {
        type        symmetry;
    }
    symmetry2
    {
        type        symmetry;
    }
    nozzleWalls
    {
	    type        compressible::alphatWallFunction;
	    value		uniform 0;
    }

}

