writeMesh
{
	type			writeObjects;
	libs			(utilityFunctionObjects);
	enabled			true;

	objects			(boundary faces faceZones neighbour owner points cellLevel pointLevel);
	writeControl	runTime;
	writeInterval   $writeRestartEvery;
}

writeRestart
{
	type			writeObjects;
	libs			(utilityFunctionObjects);
	enabled			true;

	objects			(alpha.melt alphat k nut p p_rgh T U refinementHistory);
	writeControl	runTime;
	writeInterval   $writeRestartEvery;
}

