Ugas
{
	type		multiply;
	libs		(fieldFunctionObjects);
	fields		(alpha.gas U);

	result 		Ugas;
	enabled		true;
	executeControl	timeStep;
	executeInterval	1;
	writeControl	writeTime;
}

