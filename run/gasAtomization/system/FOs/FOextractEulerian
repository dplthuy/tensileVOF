dropletPlane_20mm
{
	type	extractEulerianParticlesAMR;
	libs	(myFieldFunctionObjects);
	faceZone Hplane_20mm;
	alpha	alpha.melt;
	alphaThreshold 1e-8;

	writePrecision	6;
	writeToFile	true;
	useUserTime	false;
	enabled		true;
	log		true;
	writeControl	writeTime;
}

