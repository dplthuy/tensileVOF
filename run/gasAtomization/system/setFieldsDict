#include "./initialConditions"

defaultFieldValues
();

regions
(
    cylinderToCell
    {
    	p1	    (0 0 55.6e-3);
    	p2	    (0 0 31.4e-3);
        radius  1.25e-3;
    	
    	fieldValues
    	(
    		volScalarFieldValue alphat 0.0
            volScalarFieldValue k 0.0
            volScalarFieldValue nut 0.0
            volScalarFieldValue p_rgh $pAtm
    		volScalarFieldValue p $pAtm
            volScalarFieldValue T $meltInletTemperature
    		volVectorFieldValue U (0 0 -5.1)
    	);
    }
);


